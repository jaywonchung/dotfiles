[user]
  email = jwnchung@umich.edu
  name = Jae-Won Chung
[alias]
  last = log -1 HEAD
  unstage = reset HEAD --
[core]
  editor = nvim
	autocrlf = false
[diff]
  tool = vimdiff
[merge]
  tool = fugitive
[difftool]
  prompt = false
  trustExitCode = true
[difftool "vimdiff"]
  path = nvim
[mergetool "fugitive"]
  cmd = nvim -f -c "Gvdiffsplit!" "$MERGED"
[pull]
	ff = only
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
